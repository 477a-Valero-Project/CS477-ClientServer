<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Logic.Database">
    <class name="Record" table="Records">
        <id name="recordId" column="recordId">
            <generator class="native"/>
        </id>
        <property name="patientId" column="patientId" type="int" not-null="true"></property>
        <property name="doctorId" column="doctorId" type="int" not-null="false"></property>
        <property name="filePathRaw" column="filePathRaw" type="string"></property>
        <property name="filePathProcess" column="filePathProcess" type="string"></property>

    </class>
</hibernate-mapping>